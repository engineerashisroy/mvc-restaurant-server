{
    "version": 2,
    "regions": ["sfo1", "cdg1"],
    "builds": [
      {
        "src": "./src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./src/index.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }
  